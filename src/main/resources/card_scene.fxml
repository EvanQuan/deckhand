<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER"
          gridLinesVisible="true" hgap="20.0" minHeight="300.0" minWidth="257.0"
          prefHeight="300.0" prefWidth="298.0" vgap="20.0"
          xmlns="http://javafx.com/javafx/8.0.172-ea"
          fx:controller="com.github.evanquan.deckhand.gui.Controller">
    <columnConstraints>
        <ColumnConstraints maxWidth="45.0" minWidth="5.0" prefWidth="24.0"/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="25.0" minHeight="0.0" prefHeight="0.0"/>
    </rowConstraints>
    <Label fx:id="warningLabel" contentDisplay="CENTER" text="Title"
           textAlignment="CENTER" GridPane.columnSpan="2"
           GridPane.halignment="CENTER" GridPane.valignment="BOTTOM"/>
    <Button fx:id="warningConfirmationButton" alignment="CENTER"
            contentDisplay="CENTER" minWidth="44.0" mnemonicParsing="false"
            onAction="#confirmQuit" prefHeight="25.0" prefWidth="44.0"
            text="Discard" textAlignment="CENTER" GridPane.rowIndex="1"
            GridPane.valignment="TOP"/>
    <Button fx:id="warningConfirmationButton1" alignment="CENTER"
            contentDisplay="CENTER" layoutX="137.0" layoutY="170.0"
            minWidth="44.0" mnemonicParsing="false"
            onAction="#confirmQuit" prefHeight="25.0" prefWidth="44.0"
            text="Place " textAlignment="CENTER" GridPane.columnIndex="1"
            GridPane.rowIndex="1" GridPane.valignment="TOP"/>
</GridPane>
