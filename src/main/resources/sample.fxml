<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" hgap="20.0"
          minHeight="250.0" minWidth="600.0" prefHeight="250.0"
          prefWidth="600.0" vgap="20.0"
          xmlns="http://javafx.com/javafx/8.0.172-ea"
          fx:controller="com.github.evanquan.deckhand.gui.Controller">
    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints/>
        <RowConstraints maxHeight="25.0" minHeight="0.0" prefHeight="25.0"/>
        <RowConstraints maxHeight="25.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="25.0" minHeight="0.0" prefHeight="0.0"/>
    </rowConstraints>
    <Label contentDisplay="CENTER" text="Deck" textAlignment="CENTER"
           GridPane.columnIndex="1" GridPane.columnSpan="2"
           GridPane.halignment="CENTER" GridPane.rowIndex="4"/>
    <ProgressBar prefHeight="18.0" prefWidth="120.0" progress="0.0"
                 GridPane.columnIndex="1" GridPane.columnSpan="2"
                 GridPane.rowIndex="5"/>
    <Button fx:id="btn_msg" alignment="CENTER" contentDisplay="CENTER"
            minWidth="50.0" mnemonicParsing="false" onAction="#pressButton"
            prefWidth="50.0" text="Draw" textAlignment="CENTER"
            GridPane.columnIndex="1" GridPane.rowIndex="9"/>
    <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0"
            text="Shuffle" GridPane.columnIndex="1" GridPane.columnSpan="2"
            GridPane.rowIndex="7"/>
    <TextField minWidth="50.0" prefHeight="25.0" prefWidth="50.0"
               GridPane.columnIndex="2" GridPane.rowIndex="9"/>
    <Button minWidth="50.0" mnemonicParsing="false" prefHeight="25.0"
            prefWidth="53.0" text="Undo" GridPane.columnIndex="2"
            GridPane.rowIndex="2"/>
    <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0"
            text="Shuffle" GridPane.columnIndex="4" GridPane.columnSpan="2"
            GridPane.rowIndex="7"/>
    <Button minWidth="50.0" mnemonicParsing="false" prefWidth="50.0" text="Draw"
            GridPane.columnIndex="4" GridPane.rowIndex="9"/>
    <TextField minWidth="50.0" prefHeight="25.0" prefWidth="50.0"
               GridPane.columnIndex="5" GridPane.rowIndex="9"/>
    <TextArea editable="false" minWidth="50.0" prefHeight="85.0"
              prefWidth="300.0" GridPane.columnIndex="3" GridPane.rowIndex="3"
              GridPane.rowSpan="8"/>
    <ProgressBar prefHeight="18.0" prefWidth="120.0" progress="0.0"
                 GridPane.columnIndex="4" GridPane.columnSpan="2"
                 GridPane.rowIndex="5"/>
    <Label text="Discards" GridPane.columnIndex="4" GridPane.columnSpan="2"
           GridPane.halignment="CENTER" GridPane.rowIndex="4"/>
    <Label contentDisplay="CENTER" text="History" GridPane.columnIndex="3"
           GridPane.halignment="CENTER" GridPane.rowIndex="2"/>
    <Button minWidth="50.0" mnemonicParsing="false" prefHeight="25.0"
            prefWidth="51.0" text="Reset" GridPane.columnIndex="1"
            GridPane.rowIndex="2"/>
    <ScrollBar orientation="VERTICAL" prefWidth="14.0" GridPane.columnIndex="3"
               GridPane.rowIndex="3" GridPane.rowSpan="8"/>
</GridPane>
